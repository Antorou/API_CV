{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api-docs/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/swagger.json",
      "dest": "swagger.json"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}